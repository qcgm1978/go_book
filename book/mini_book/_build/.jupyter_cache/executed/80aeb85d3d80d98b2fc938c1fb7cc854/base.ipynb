    "# Listing 3.1. Using an enum to represent players\n",
    "\n",
    "import enum\n",
    "\n",
    "\n",
    "class Player(enum.Enum):\n",
    "    black = 1\n",
    "    white = 2\n",
    "\n",
    "    @property\n",
    "    def other(self):\n",
    "        return Player.black if self == Player.white else Player.white"
